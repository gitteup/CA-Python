from math import sqrt

print sqrt(13689)
        
 #
 # Built-In Functions
 #
        
        
def distance_from_zero(zero):
    if type(zero) == int or type(zero) == float:
        return abs(zero)
    else:
        return "Nope"
        
 #
 # Take a Vacation
 #        
        
        
def hotel_cost(nights):
    return 140 * nights

def plane_ride_cost(city):
    if city == "Charlotte":
        return 183
    elif city == "Tampa":
        return 220
    elif city == "Pittsburgh":
        return 222
    elif city == "Los Angeles":
        return 475
